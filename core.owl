    <!-- https://w3id.org/polifonia/ontology/core/Address -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/core/Address">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/GeographicalFeature"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/hasAddressComponent"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/core/AddressComponent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/postcode"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/streetNumber"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Address</rdfs:label>
    </owl:Class>
    


